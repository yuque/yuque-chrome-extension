{
  "name": "yuque-chrome-extension",
  "version": "0.2.1",
  "description": "Yuque browser extension for Chrome/Edge",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/yuque/yuque-chrome-extension.git"
  },
  "issues": "https://www.yuque.com/yuque/yuque-chrome-extension/welcome",
  "scripts": {
    "archive": "sh ./build.sh",
    "bundle": "NODE_ENV=production webpack --mode=production",
    "build": "npm run bundle && npm run archive",
    "contributor": "git-contributor",
    "dev": "NODE_ENV=development webpack --mode=development",
    "docs:dev": "vuepress dev docs",
    "type:check": "tsc --noEmit",
    "lint:js": "eslint . --fix",
    "lint:less": "stylelint --fix ./**/*.less -s less",
    "lint": "npm run lint:js && npm run lint:less",
    "lint-staged": "lint-staged",
    "translate": "easy-i18n-cli -c ./i18n.config.js",
    "translate:check": "npm run translate -- --check",
    "prepare": "husky install",
    "release": "standard-version && git push origin --tags"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.13.7",
    "@mui/material": "^5.13.7",
    "antd": "^4.24.12",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "escape-html": "^1.0.3",
    "eventemitter3": "^4.0.7",
    "jquery": "^3.6.0",
    "less": "^4.1.2",
    "lodash": "^4.17.21",
    "marked": "^2.0.7",
    "moment": "^2.29.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semver-compare": "^1.0.0",
    "slate": "^0.63.0",
    "slate-history": "^0.62.0",
    "slate-hyperscript": "^0.62.0",
    "slate-react": "^0.65.0",
    "turndown": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "7.8.3",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@types/chrome": "^0.0.224",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/semver-compare": "^1.0.1",
    "@types/webpack": "^5.28.1",
    "@types/webpack-env": "^1.18.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.6",
    "chai": "^4.1.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "6",
    "cross-env": "^7.0.3",
    "css-loader": "^6.6.0",
    "dotenv": "^10.0.0",
    "easy-i18n-cli": "^1.0.0",
    "eslint": "^8.45.0",
    "eslint-config-egg": "^12.2.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "git-contributor": "^2.1.5",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "jsx-loader": "^0.13.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.4",
    "standard-version": "^9.5.0",
    "style-loader": "^3.3.2",
    "stylelint": "^12.0.1",
    "svg-sprite-loader": "^4.1.6",
    "svgo": "^1.3.2",
    "svgo-loader": "^2.2.1",
    "ts-loader": "^8.4.0",
    "typescript": "^4.9.5",
    "vuepress": "^1.8.2",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.7.4"
  },
  "optionalDependencies": {
    "copy-paste": "^1.3.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*/*.less": [
      "stylelint --fix -s less"
    ]
  }
}
