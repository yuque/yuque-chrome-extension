    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.13.7",
    "@mui/material": "^5.13.7",
    "antd": "4.8.x",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "escape-html": "^1.0.3",
    "eventemitter3": "^4.0.7",
    "jquery": "^3.6.0",
    "less": "^4.1.2",
    "lodash": "^4.17.21",
    "marked": "^2.0.7",
    "moment": "^2.29.4",
    "prop-types": "^15.8.1",
    "react": "^16.13.1",
    "react-dom": "16.13.1",
