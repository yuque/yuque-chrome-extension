{
  "name": "yuque-chrome-extension",
  "version": "0.2.0",
  "description": "Yuque browser extension for Chrome/Edge",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/yuque/yuque-chrome-extension.git"
  },
  "issues": "https://www.yuque.com/yuque/yuque-chrome-extension/welcome",
  "scripts": {
    "archive": "sh ./build.sh",
    "build": "NODE_ENV=production webpack --mode=production && npm run archive",
    "contributor": "git-contributor",
    "dev": "NODE_ENV=development webpack --mode=development",
    "docs:dev": "vuepress dev docs",
    "lint": "eslint . --ext .jsx,.js --fix && stylelint --fix ./**/*.less -s less",
    "translate": "easy-i18n-cli -c ./i18n.config.js",
    "translate:check": "npm run translate -- --check"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
