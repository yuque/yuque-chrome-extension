    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "7.8.3",
    "@types/chrome": "^0.0.224",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/webpack": "^5.28.1",
